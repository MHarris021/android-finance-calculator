<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.finance.bond.calculator.StudentBondCalculator" android:versionName="2.0.0" android:versionCode="2">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".StudentBondActivity"
			android:configChanges="orientation" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity android:configChanges="orientation" android:launchMode="standard"
			android:name="com.finance.bond.calculator.BondCalculator"></activity>
		<activity android:configChanges="orientation" android:launchMode="standard"
			android:name="com.finance.bond.calculator.CouponSchedule"></activity>
	<activity android:name="com.finance.bond.calculator.calculatorPreferences"></activity>
</application>


	<permission android:name="com.android.vending.CHECK_LICENSE"></permission>


</manifest> 